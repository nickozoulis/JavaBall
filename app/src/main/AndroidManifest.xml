<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.nickozoulis.teamproj" >

    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activities.ActivityViewRefereeProfile"
            android:label="@string/title_activity_activity_referee_profile" >
        </activity>
        <activity
            android:name=".activities.ActivityEditRefereeProfile"
            android:label="@string/title_activity_activity_edit_referee_profile" >
        </activity>
        <activity
            android:name=".activities.ActivityCreateReferee"
            android:label="@string/title_activity_activity_create_referee" >
        </activity>
        <activity
            android:name=".activities.ActivityCreateMatch"
            android:label="@string/title_activity_activity_create_match" >
        </activity>
        <activity
            android:name=".activities.ActivityBarChart"
            android:label="@string/title_activity_activity_bar_chart" >
        </activity>
    </application>

</manifest>
